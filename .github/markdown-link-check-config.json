{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/.*/blob/"
    },
    {
      "pattern": "^https://github.com/.*/tree/"
    }
  ],
  "replacements": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "gzip, deflate, br"
      }
    }
  ],
  "timeout": 20000,
  "retryOn429": true,
  "retryCount": 3,
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308, 404]
}
